{
    "name": "Flex Marks Sum",
    "version": "0.1.0",
    "description": "Sums up each course marks",
    "permissions": [
        "activeTab",
        "debugger",
        "tabs"
    ],
    "host_permissions": ["https://flexstudent.nu.edu.pk/Student/StudentMarks?*"],
    "background": {
      "service_worker": "background.js"
    },
    "developer":
    {
        "name": "WAHAJ JAVED"
    },
    "content_scripts": [
      {
        "matches": ["https://flexstudent.nu.edu.pk/Student/StudentMarks?*"],
        "js": ["contentScript.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/bookmark.png",
          "assets/play.png",
          "assets/delete.png",
          "assets/save.png"
        ],
        "matches": ["https://flexstudent.nu.edu.pk/Student/StudentMarks?*"]
      }
    ],
    "action": {
      "default_icon": {
        "16": "assets/ext-icon.png",
        "24": "assets/ext-icon.png",
        "32": "assets/ext-icon.png"
      },
      "default_title": "My YT Bookmarks",
      "default_popup": "popup.html"
    },
    "manifest_version": 3
}